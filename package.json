{
    "name": "ayyo",
    "version": "0.2.0",
    "description": "A lightweight all-in-one web router. MEEP",
    "main": "lib/index.mjs",
    "exports": "lib/index.mjs",
    "module": "lib/index.mjs",
    "type": "module",
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "style": "xo",
        "test": "nyc ava",
        "performance": "k6 run tests/performance.k6.js",
        "benchmark": "h2load -n1000000 -c1000 -m10 -t 7 https://localhost:8080",
        "install-peers": "install-peers -f"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/FallingSnow/ayyo.git"
    },
    "keywords": [
        "web",
        "framework",
        "http2",
        "router",
        "openapi",
        "swagger"
    ],
    "author": "Ayrton Sparling <snowboarding96@yahoo.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FallingSnow/ayyo/issues"
    },
    "homepage": "https://github.com/FallingSnow/ayyo#readme",
    "devDependencies": {
        "@ava/babel": "^1.0.1",
        "acorn": "^7.3.1",
        "ava": "^3.9.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.4.0",
        "form-data": "^3.0.0",
        "got": "^11.3.0",
        "husky": "^4.2.5",
        "install-peers-cli": "^2.2.0",
        "joigoose": "^4.0.4",
        "mongoose": "^5.4.20",
        "nodemon": "^2.0.4",
        "nyc": "^15.1.0",
        "trace": "^3.1.1",
        "xo": "^0.32.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run style"
        }
    },
    "xo": {
        "semicolon": true,
        "space": 2,
        "extends": [
            "./.eslintrc.json"
        ],
        "ignores": [
            "./lib/middleware/metrics/app"
        ]
    },
    "ava": {
        "babel": true,
        "files": [
            "tests/**.spec.js"
        ]
    },
    "dependencies": {
        "@hapi/joi": "git+https://github.com/FallingSnow/joi.git#feature/validation-strip-option",
        "fast-stable-stringify": "^1.0.0",
        "file-type": "^12.1.0",
        "http-status": "^1.3.1",
        "lodash.throttle": "^4.1.1",
        "luxon": "^2.5.2",
        "mime": "^2.4.6",
        "path-to-regexp": "^6.1.0",
        "readable-stream-clone": "^0.0.7",
        "selfsigned": "^1.10.7",
        "simple-cookie": "^1.0.10",
        "traverse": "^0.6.6"
    },
    "peerDependencies": {
        "jsonwebtoken": "^8.5.1",
        "load-balancers": "^1.3.52",
        "maxmind": "^4.1.3",
        "prettier": "^2.0.5",
        "sharedmap": "^1.1.1",
        "tmp": "^0.2.1",
        "ua-parser-js": "^0.7.21",
        "xxhash": "^0.3.0"
    }
}
